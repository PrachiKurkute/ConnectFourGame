package edu.nyu.pqs.connectfour;
/**
 * Class DemoConnectFour used to simulate the Game Connect Four.
 * @author Prachi Kurkute
 *
 */
public class DemoConnectFour {
	public void start() {
		ConnectFourGame connect_four = new ConnectFourGame.Builder().row(6).column(7).getSingleInstance();
		ConnectFourUI connect_four_ui = new ConnectFourUI(connect_four);
	}
	/**
	 * Program execution begins here
	 * @param args
	 */
	public static void main(String args[]) {
		DemoConnectFour demo = new DemoConnectFour();
		demo.start();
	}
}

























































